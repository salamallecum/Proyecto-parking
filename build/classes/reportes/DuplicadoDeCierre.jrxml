	<parameter name="codigo" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     cierres.`Codigo` AS cierres_Codigo,
     cierres.`Nombre_usuario` AS cierres_Nombre_usuario,
     cierres.`Producido` AS cierres_Producido,
     cierres.`Dinero_en_caja` AS cierres_Dinero_en_caja,
     cierres.`Diferencia` AS cierres_Diferencia,
     cierres.`No_facturas` AS cierres_No_facturas,
     cierres.`Observaciones` AS cierres_Observaciones,
     facturas.`Codigo` AS facturas_Codigo,
     facturas.`Placa` AS facturas_Placa,
     facturas.`Contabilizada` AS facturas_Contabilizada,
     facturas.`Valor_a_pagar` AS facturas_Valor_a_pagar,
     facturas.`Efectivo` AS facturas_Efectivo,
     facturas.`Cambio` AS facturas_Cambio,
     facturas.`Id_cierre` AS facturas_Id_cierre,
     cierres.`Id_cierre` AS cierres_Id_cierre
FROM
     `cierres` cierres INNER JOIN `facturas` facturas ON cierres.`Id_cierre` = facturas.`Id_cierre`

WHERE
    cierres.`Codigo`=$P{codigo}
AND
facturas.`Contabilizada`='Si']]>
	</queryString>
